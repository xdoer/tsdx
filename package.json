{
  "name": "@xdoer/tsdx",
  "version": "0.0.1",
  "author": "xdoer",
  "description": "Zero-config TypeScript package development which based on Jared Palmer's tsdx",
  "license": "MIT",
  "homepage": "https://github.com/xdoer/tsdx",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xdoer/tsdx.git"
  },
  "keywords": [
    "react",
    "typescript",
    "bundle",
    "rollup",
    "tsdx"
  ],
  "bugs": {
    "url": "https://github.com/xdoer/tsdx/issues"
  },
  "bin": {
    "tsdx": "./dist/index.js"
  },
  "scripts": {
    "prepare": "tsc -p tsconfig.json & husky install",
    "build": "tsc -p tsconfig.json",
    "lint": "yarn build && yarn lint:post-build",
    "lint:post-build": "node dist/index.js lint ./ --ignore-pattern 'test/e2e/fixtures/lint'",
    "test": "yarn build && yarn test:post-build",
    "test:post-build": "node dist/index.js test",
    "start": "tsc -p tsconfig.json --watch",
    "release": "np",
    "deduplicate": "yarn-deduplicate -s fewer yarn.lock",
    "deduplicate:check": "yarn-deduplicate -s fewer yarn.lock --list --fail"
  },
  "files": [
    "dist",
    "templates"
  ],
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@babel/helper-module-imports": "^7.0.0",
    "@babel/parser": "^7.11.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/traverse": "^7.11.5",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.2.1",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "ansi-escapes": "^4.2.1",
    "asyncro": "^3.0.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-polyfill-regenerator": "^0.2.2",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "camelcase": "^6.0.0",
    "chalk": "^4.0.0",
    "enquirer": "^2.3.4",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.8.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^4.2.0",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "jest": "^27.0.6",
    "jest-watch-typeahead": "^0.6.4",
    "jpjs": "^1.2.1",
    "lodash.merge": "^4.6.2",
    "ora": "^5.4.1",
    "pascal-case": "^3.1.1",
    "prettier": "^2.3.2",
    "progress-estimator": "^0.3.0",
    "regenerator-runtime": "^0.13.7",
    "rollup": "^2.52.8",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "sade": "^1.4.2",
    "semver": "^7.1.1",
    "shelljs": "^0.8.5",
    "tiny-glob": "^0.2.6",
    "ts-jest": "^27.0.3",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.14",
    "@types/fs-extra": "^9.0.1",
    "@types/lodash": "^4.14.161",
    "@types/node": "^16.0.1",
    "@types/react": "^17.0.14",
    "@types/rollup-plugin-json": "^3.0.2",
    "@types/sade": "^1.6.0",
    "@types/semver": "^7.1.0",
    "@types/shelljs": "^0.8.5",
    "@types/styled-components": "^5.0.1",
    "autoprefixer": "^9.7.4",
    "babel-plugin-replace-identifiers": "^0.1.1",
    "cssnano": "^4.1.10",
    "doctoc": "^2.0.1",
    "husky": "^7.0.1",
    "np": "^7.5.0",
    "pretty-quick": "^3.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "rollup-plugin-postcss": "^2.5.0",
    "styled-components": "^5.0.1",
    "tiny-invariant": "^1.1.0",
    "tiny-warning": "^1.0.3",
    "yarn-deduplicate": "^3.1.0"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
